      "parameters": {
        "jsCode": "// Analyse hebdomadaire des donnÃ©es\nconst weekData = $input.all();\n\n// Calculs statistiques\nconst healthScores = weekData.map(d => d.json.health_score);\nconst avgHealth = healthScores.reduce((a,b) => a+b, 0) / healthScores.length;\nconst minHealth = Math.min(...healthScores);\nconst maxHealth = Math.max(...healthScores);\n\n// Tendances revenus\nconst revenues = weekData.map(d => d.json.revenue);\nconst weekRevenue = revenues.reduce((a,b) => a+b, 0);\nconst avgDailyRevenue = weekRevenue / 7;\n\n// Performance metrics\nconst conversions = weekData.map(d => d.json.conversions);\nconst totalConversions = conversions.reduce((a,b) => a+b, 0);\n\n// Identification patterns\nconst patterns = {\n  bestDay: '',\n  worstDay: '',\n  peakHours: [],\n  criticalEvents: 0\n};\n\n// Recommendations basÃ©es sur l'analyse\nconst weeklyInsights = {\n  summary: {\n    avgHealthScore: Math.round(avgHealth),\n    healthRange: `${minHealth}-${maxHealth}`,\n    totalRevenue: weekRevenue,\n    avgDailyRevenue: Math.round(avgDailyRevenue),\n    totalConversions,\n    conversionTrend: totalConversions > 350 ? 'Positive' : 'Ã€ amÃ©liorer'\n  },\n  patterns,\n  recommendations: [],\n  nextWeekForecast: {\n    expectedRevenue: Math.round(avgDailyRevenue * 7 * 1.1),\n    expectedConversions: Math.round(totalConversions * 1.05),\n    confidence: '85%'\n  }\n};\n\nreturn weeklyInsights;"
      },
